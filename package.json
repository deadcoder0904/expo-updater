{
	"name": "expo-updater",
	"version": "0.0.5",
	"description": "Update package.json to use latest Expo SDK",
	"license": "MIT",
	"repository": "deadcoder0904/expo-updater",
	"author": {
		"name": "Akshay Kadam",
		"email": "akshaykadam0904@gmail.com",
		"url": "http://twitter.com/deadcoder0904"
	},
	"engines": {
		"node": ">=4"
	},
	"scripts": {
		"precommit": "lint-staged",
		"start": "node index.js"
	},
