	"preferGlobal": true,
	"dependencies": {
		"@expo/json-file": "^5.3.0",
		"async": "^2.6.0",
		"gh-got": "^7.0.0",
		"got": "^8.0.1",
		"package-json": "^4.0.1"
	},
	"devDependencies": {
		"husky": "^0.14.3",
		"lint-staged": "^6.0.0",
		"prettier": "^1.9.2"
